{
  "name": "flowershop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:selenium1": "node tests/test1.js",
    "test:selenium2": "node tests/test2.js",
    "test:selenium3": "node tests/test3.js",
    "test:bdd": "cucumber-js tests/bdd/features/**/*.feature --require tests/bdd/step_definitions/**/*.steps.js",
    "test:bdd:anetari1:product": "cucumber-js tests/bdd/features/anetari1/product_validation.feature --require tests/bdd/step_definitions/anetari1/product_validation.steps.js",
    "test:bdd:anetari1:category": "cucumber-js tests/bdd/features/anetari1/category_limit.feature --require tests/bdd/step_definitions/anetari1/category_limit.steps.js",
    "test:bdd:anetari1:admin": "cucumber-js tests/bdd/features/anetari1/admin_count.feature --require tests/bdd/step_definitions/anetari1/admin_count.steps.js",
    "test:bdd:anetari1:contact": "cucumber-js tests/bdd/features/anetari1/contact_form.feature --require tests/bdd/step_definitions/anetari1/contact_form.steps.js",
    "test:bdd:anetari1:all": "npm run test:bdd:anetari1:product && npm run test:bdd:anetari1:category && npm run test:bdd:anetari1:admin && npm run test:bdd:anetari1:contact",
    "test:bdd:anetari2:price": "cucumber-js tests/bdd/features/anetari2/price_validation.feature --require tests/bdd/step_definitions/anetari2/price_validation.steps.js",
    "test:bdd:anetari2:stock": "cucumber-js tests/bdd/features/anetari2/stock_validation.feature --require tests/bdd/step_definitions/anetari2/stock_validation.steps.js",
    "test:bdd:anetari2:contact-email": "cucumber-js tests/bdd/features/anetari2/contact_email.feature --require tests/bdd/step_definitions/anetari2/contact_email.steps.js",
    "test:bdd:anetari2:contact-message": "cucumber-js tests/bdd/features/anetari2/contact_message.feature --require tests/bdd/step_definitions/anetari2/contact_message.steps.js",
    "test:bdd:anetari2:all": "npm run test:bdd:anetari2:price && npm run test:bdd:anetari2:stock && npm run test:bdd:anetari2:contact-email && npm run test:bdd:anetari2:contact-message",
    "test:bdd:anetari3:user-profile": "cucumber-js tests/bdd/features/anetari3/user_profile.feature --require tests/bdd/step_definitions/anetari3/user_profile.steps.js",
    "test:bdd:anetari3:category": "cucumber-js tests/bdd/features/anetari3/category_management.feature --require tests/bdd/step_definitions/anetari3/category_management.steps.js",
    "test:bdd:anetari3:user-management": "cucumber-js tests/bdd/features/anetari3/user_management.feature --require tests/bdd/step_definitions/anetari3/user_management.steps.js",
    "test:bdd:anetari3:message": "cucumber-js tests/bdd/features/anetari3/message_management.feature --require tests/bdd/step_definitions/anetari3/message_management.steps.js",
    "test:bdd:anetari3:all": "npm run test:bdd:anetari3:user-profile && npm run test:bdd:anetari3:category && npm run test:bdd:anetari3:user-management && npm run test:bdd:anetari3:message",
    "test:bdd:all": "npm run test:bdd:anetari1:all && npm run test:bdd:anetari2:all && npm run test:bdd:anetari3:all"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "chromedriver": "^137.0.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0",
    "selenium-webdriver": "^4.31.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "chai": "^4.3.10",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0"
  }
}
